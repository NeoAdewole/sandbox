{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Sandbox Blocky",
  "name": "neo-sandbox/blocky",
  "version": "0.1.0",
  "category": "widgets",
  "icon": "admin-generic",
  "description": "Gutenberg block playground for the Sandbox theme.",
  "keywords": [
    "sandbox",
    "blocky",
    "playground"
  ],
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "blocky",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./index.css"
}